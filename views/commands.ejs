    <tr>
     <th>Name</th>
     <th>Description</th> 
     <th>Usage</th>
    </tr>
<% let Fun = cmd.filter(a => a.category === 'Fun')
Fun.map(x => { %>
    <tr>
     <td><%=x.name%></td>
     <td><%=x.description%></td>
     <td><%=x.usage.replace('`', '').replace('`', '')%></td>
