   <main> 
    <div class="fitur"><b>Partner List</b>
      <hr style="width: 80%; margin-bottom: 15px;">
    </div>
     <div class="partner-top"> 
        <% let partner = db.get(`partner`) 
        
          if(partner) {
          partner.map(x => {
          
          let guild = bot.guilds.cache.get(x.id)
          if(!guild) return; %>
         <div class="partner">
          <img src="<%=guild.iconURL() ? guild.iconURL({dynamic: true, format: 'png'}) : 'https://cdn.discordapp.com/embed/avatars/0.png'%>"  draggable="false">
          <h4> <%=guild.name.length > 25 ? guild.name.slice(0, 25) + '...' : guild.name%></h4>
          <a class="partner-link" href="/partner/<%=guild.id%>">Show</a>
          <a class="partner-link" href="/partner/<%=guild.id%>/invite">Join</a>
         </div>
          <%})} else{ %>     
       <h1>None Partner</h1>
          <%}%>
     </div>     
   </main>
           <br>
  <%- include('../partials/footer')%>
  </body>
</html>