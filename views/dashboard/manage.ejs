 <%if(!user) {%>
  <div id="myModal" class="modal">
   <div class="modal-content">
     <p style="margin-bottom: 30px;">You must login to acces this page</p>
     <a href="/login" class="partner-link abc">Login</a>
     <a href="/" class="partner-link abc">Cancel</a>
    </div>
   </div>
<%} else {%>
    <div class="fitur"><b>Server List</b>
      <hr style="width: 80%; margin-bottom: 15px;">
    </div>
     <div class="partner-top"> 
<% if() {%>
     <h1 style="color: white; margin: 10% 0%; text-align: center; text-shadow: 3px 2px 6px black">You do not have any server with Manage Server Permissions</h1>
<%} else {
   user.guilds.forEach(guild => {     
     if (!client.guilds.cache.get(guild.id) || 
         !client.guilds.cache.get(guild.id).me.hasPermission('MANAGE_GUILD') || 
         !client.guilds.cache.get(guild.id).members.cache.get(user.id).hasPermission('MANAGE_GUILD')) return;
         
                let img = "https://cdn.discordapp.com/icons/" + guild.id + "/" + guild.icon + ""
                if(!guild.icon) img = "https://cdn.discordapp.com/embed/avatars/0.png"; %>
        <div class="partner">
         <img src="<%=img%>">
         <h4><%= guild.name.length > 15 ? guild.name.slice(0, 15) + '...' : guild.name %></h4>
         <p><%=guild.memberCount%></p>
         <a href="/manage/<%=guild.id%>" class="partner-link">Manage</a>
        </div>
<% }) }%>
     </div>   
    <div class="footer">
     <div class="footer-raw">
      <div class="footer-main">
