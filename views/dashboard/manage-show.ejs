  <header style="height:80px; padding-top: 0px;">
     <h4 style="padding-top: 30px;text-align: center; color: white; background-color:#1a1b1d; margin: 0;"><%=guild.name%></h4>  
  </header>
<img class="border" height="50" src="https://media.discordapp.net/attachments/788251058630688802/789157945706807366/-ay.png" draggable="false">
    <div class="fitur"><b>Configuration</b>
      <hr style="width: 80%; margin-bottom: 15px;">
    </div>
  <div class="manage-top">
    <div onclick="window.location.href='/manage/<%=guild.id%>/custom-commands';" class="manage-main">
      <div class="manage-image">
       <svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100px" fill="currentColor" class="bi bi-card-text" viewBox="6 0 4 16">
        <path d="M14.5 3a.5.5 0 0 1 .5.5v9a.5.5 0 0 1-.5.5h-13a.5.5 0 0 1-.5-.5v-9a.5.5 0 0 1 .5-.5h13zm-13-1A1.5 1.5 0 0 0 0 3.5v9A1.5 1.5 0 0 0 1.5 14h13a1.5 1.5 0 0 0 1.5-1.5v-9A1.5 1.5 0 0 0 14.5 2h-13z"/>
