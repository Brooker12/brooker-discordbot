        <path fill-rule="evenodd" d="M7.646 11.854a.5.5 0 0 0 .708 0l3-3a.5.5 0 0 0-.708-.708L8.5 10.293V1.5a.5.5 0 0 0-1 0v8.793L5.354 8.146a.5.5 0 1 0-.708.708l3 3z"/>
       </svg>
      </div>
      <div class="manage-text">
        <h1>Leave</h1>
        <p>Send a goodbye message when there left members</p>
      </div>
    </div>
    <div onclick="window.location.href='/manage/<%=guild.id%>/leveling';" class="manage-main">
      <div class="manage-image">
       <svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100px" fill="currentColor" class="bi bi-arrow-down-up" viewBox="6 0 4 16">
        <path fill-rule="evenodd" d="M11.5 15a.5.5 0 0 0 .5-.5V2.707l3.146 3.147a.5.5 0 0 0 .708-.708l-4-4a.5.5 0 0 0-.708 0l-4 4a.5.5 0 1 0 .708.708L11 2.707V14.5a.5.5 0 0 0 .5.5zm-7-14a.5.5 0 0 1 .5.5v11.793l3.146-3.147a.5.5 0 0 1 .708.708l-4 4a.5.5 0 0 1-.708 0l-4-4a.5.5 0 0 1 .708-.708L4 13.293V1.5a.5.5 0 0 1 .5-.5z"/>
