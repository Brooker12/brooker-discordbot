  <li class="login"><a href="login">Login</a></li>     
  <% } %>
    <li class="icon">
     <a href="javascript:void(0);" onclick="myFunction()">â˜°</a>  
    </li>
   </ul> 
  </nav>
<header style="height: 50px; margin-bottom: 5%;"><h1>Custom Commands</h1></header>
    <div style="width: 80%;display: flex;margin: auto;flex-wrap: wrap;justify-content: center;height: auto;border: 0px solid;">
     <div class="test">
      <h3 style="margin-bottom: 0; text-align: center;">Add Custom Command</h3>
       <hr style="width: 55%;">
       <br>
      <form style="margin: auto 100px 25px; text-align: center;" action="/manage/<%=guild.id%>/custom-commands" method="POST">
       <label for="Custom">Name</label><br>
        <input style="width: 250px;" type="text" id="name" name="name" placeholder="Name Command">
          <br>
         <label for="Custom">Response</label><br>
        <input style="width: 250px;" type="text" id="responce" name="responce" placeholder="Command Response">
         <br>
        <input type="submit" value="Submit">
       </form>
      </div>
      <div class="test">
       <h3 style="margin-bottom: 0; text-align: center;">Custom Command List</h3>
       <hr style="width: 55%;">
        <br>
        <div style="margin: auto 100px 20px; overflow-x: auto; border: none;">
          <% let cmd = db.get(`cmd_${guild.id}`)
          
          if(cmd && cmd.length) { %>
           <table id="t01">
            <tr>
             <th>Name</th>
             <th>Responce</th> 
            </tr>
             <%cmd.map(x => { %>
              <tr>
               <td><%=x.name%></td>
               <td><%=x.responce%></td>
              </tr>
             <%})%>
            </table>
          <%}else{%>
          <h2>Nothing custom command has added</h2>
          <%}%>
        </div>
      </div>
    </div>
  </body>
</html>